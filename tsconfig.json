{
	"compilerOptions": {
		"downlevelIteration": true,
		"target": "ESNext",
		"module": "NodeNext",
		"lib": ["ESNext", "DOM"],
		"strict": true,
		"moduleResolution": "NodeNext",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"sourceMap": true,
		"skipLibCheck": true,
		"resolveJsonModule": true
	}
}
